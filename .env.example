# YouTube API Configuration
# Obtenha sua API key em: https://console.cloud.google.com/apis/credentials
VITE_YOUTUBE_API_KEY=your_youtube_api_key_here

# OpenAI Configuration (para geração de roadmap)
# Obtenha sua API key em: https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Application Configuration
VITE_APP_NAME=NeuroTrack
VITE_APP_VERSION=1.0.0

# Supabase Configuration
# Obtenha suas credenciais em: https://app.supabase.com/project/_/settings/api
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Feature Flags
VITE_ENABLE_YOUTUBE=true
VITE_ENABLE_WIKIPEDIA=true
VITE_ENABLE_AI_ROADMAP=true
VITE_ENABLE_SUPABASE=false
